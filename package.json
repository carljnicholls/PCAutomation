{
    "name": "win10automation",
    "version": "0.0.1",
    "description": "Listens to pushbullet messages and runs automation tasks",
    "main": "index.js",
    "scripts": {
        "run-param-lock": "tsc -b && copyfiles .\\src\\scripts\\* .\\build\\src\\scripts && node .\\build\\src\\main.js lock",
        "run-param-server": "tsc -b && copyfiles .\\src\\scripts\\* .\\build\\src\\scripts && node .\\build\\src\\main.js server",
        "test": "jest",
        "copy-scripts-to-build": "copyfiles .\\src\\scripts\\* .\\build\\"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/carljnicholls/Win10Automation.git"
    },
