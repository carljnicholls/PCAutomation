        "url": "https://github.com/carljnicholls/Win10Automation/issues"
    },
    "homepage": "https://github.com/carljnicholls/Win10Automation#readme",
    "dependencies": {
        "app-root-path": "^3.0.0",
        "dotenv": "^8.2.0",
        "execa": "^4.0.1",
        "fs-extra": "^9.0.0",
        "lock-system": "^2.0.0",
        "moment": "^2.24.0",
        "node-fetch": "^2.6.0",
        "pushbullet": "^2.4.0",
        "robotjs": "^0.6.0",
        "tslib": "^1.10.0",
        "vol": "^3.0.0",
        "winston": "^3.2.1",
        "words-to-numbers": "^1.5.1",
        "ws": "^7.2.1"
    },
    "devDependencies": {
