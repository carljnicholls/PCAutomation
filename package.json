        "@types/jest": "^24.0.25",
        "@types/jest-expect-message": "^1.0.1",
        "@types/lock-system": "^2.0.0",
        "@types/moment": "^2.13.0",
        "@types/node": "^13.1.1",
        "@types/node-fetch": "^2.5.4",
        "@types/winston": "^2.4.4",
        "@types/words-to-numbers": "^1.2.1",
        "@types/ws": "^6.0.4",
        "copyfiles": "^2.2.0",
        "jest": "^24.9.0",
